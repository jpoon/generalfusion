{
  "pipeline": {
    "name": "scale"
  },
  "transform": {
    "cmd": [
      "dotnet",
      "scale.dll",
      "/pfs/calibration",
      "/pfs/raw",
      "/pfs/out"
    ],
    "image": "japoon.azurecr.io/generalfusion/scale:0.0.5",
    "imagePullSecrets": [
      "acr-secret"
    ]
  },
  "inputs": [
    {
      "repo": {
        "name": "raw"
      },
      "glob": "/*"
    },
    {
      "repo": {
        "name": "calibration"
      },
      "glob": "/"
    }
  ]
}