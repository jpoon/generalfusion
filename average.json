{
  "pipeline": {
    "name": "average"
  },
  "transform": {
    "cmd": [
      "python",
      "average.py",
      "/pfs/scale",
      "/pfs/out"
    ],
    "image": "japoon.azurecr.io/generalfusion/average:0.0.10",
    "imagePullSecrets": [
      "acr-secret"
    ]
  },
  "inputs": [
    {
      "repo": {
        "name": "scale"
      },
      "glob": "/*"
    }
  ]
}